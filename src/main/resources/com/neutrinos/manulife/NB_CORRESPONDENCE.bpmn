      <bpmn2:script><![CDATA[kcontext.setVariable("payload","");

org.json.JSONObject obj  = new org.json.JSONObject();
org.json.JSONObject decisionJson  = new org.json.JSONObject((java.lang.String)kcontext.getVariable("decisionOption"));

obj.put("cid", kcontext.getVariable("cid"));
obj.put("decision_option",decisionJson);
obj.put("decision",((java.lang.String)kcontext.getVariable("decision")));
obj.put("templates",(java.util.List)kcontext.getVariable("smsTemplates"));
obj.put("username",((java.lang.String)kcontext.getVariable("username")));
kcontext.setVariable("payload",obj.toString());

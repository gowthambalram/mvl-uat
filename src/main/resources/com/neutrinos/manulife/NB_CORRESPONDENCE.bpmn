        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[java.lang.String result = (java.lang.String)kcontext.getVariable("fetchTemplatesResponse");

org.json.JSONObject json  = new org.json.JSONObject(result);

String dataArray = json.get("data").toString();
if (dataArray != null && !dataArray.isEmpty()) {
org.json.JSONArray jsonArray = new org.json.JSONArray(dataArray);
    for(int i=0; i<jsonArray.length(); i++) {
        org.json.JSONObject jsonObject = jsonArray.getJSONObject(i);
        String type = jsonObject.getString("type");
        boolean isReminder = false;
        boolean isAgent = false;
        if (jsonObject.getJSONObject("metadata").has("is_agent")) {
        isAgent = jsonObject.getJSONObject("metadata").getBoolean("is_agent"); }
        if (jsonObject.getJSONObject("metadata").has("is_reminder")) {
        isReminder = jsonObject.getJSONObject("metadata").getBoolean("is_reminder"); }

        if (type.equals("sms") && isAgent && !isReminder) {
            String name = jsonObject.getString("name");
            kcontext.setVariable("smsTemplateAgent",name);
        } else if (type.equals("sms") && !isAgent && !isReminder) {
            String name = jsonObject.getString("name");
            kcontext.setVariable("smsTemplateClient",name);
        } else if (type.equals("email") && isAgent && !isReminder) {
            String name = jsonObject.getString("name");
            kcontext.setVariable("emailTemplateAgent",name);
        } else if (type.equals("email") && !isAgent && !isReminder) {
            String name = jsonObject.getString("name");
            kcontext.setVariable("emailTemplateClient",name);
        } else if (type.equals("sms") && isAgent && isReminder) {
            String name = jsonObject.getString("name");
            kcontext.setVariable("smsReminderTemplateAgent",name);
        } else if (type.equals("sms") && !isAgent && isReminder) {
            String name = jsonObject.getString("name");
            kcontext.setVariable("smsReminderTemplateClient",name);
        } else if (type.equals("email") && isAgent && isReminder) {
            String name = jsonObject.getString("name");
            kcontext.setVariable("emailReminderTemplateAgent",name);
        } else if (type.equals("email") && !isAgent && isReminder) {
            String name = jsonObject.getString("name");
            kcontext.setVariable("emailReminderTemplateClient",name);
        }
    }
}
System.out.println("Exit"+"cid:"+kcontext.getVariable("cid")+",node:"+kcontext.getNodeInstance().getNodeName()+",process:"+kcontext.getProcessInstance().getProcessName());]]></drools:script>

      <bpmn2:script><![CDATA[String smsTemplateAgentLoc = (java.lang.String)kcontext.getVariable("smsTemplateAgent");
String smsTemplateClientLoc = (java.lang.String)kcontext.getVariable("smsTemplateClient");
String emailTemplateAgentLoc = (java.lang.String)kcontext.getVariable("emailTemplateAgent");
String emailTemplateClientLoc = (java.lang.String)kcontext.getVariable("emailTemplateClient");
String smsReminderTemplateAgentLoc = (java.lang.String)kcontext.getVariable("smsReminderTemplateAgent");
String smsReminderTemplateClientLoc = (java.lang.String)kcontext.getVariable("smsReminderTemplateClient");
String emailReminderTemplateAgentLoc = (java.lang.String)kcontext.getVariable("emailReminderTemplateAgent");
String emailReminderTemplateClientLoc = (java.lang.String)kcontext.getVariable("emailReminderTemplateClient");

String [] emailArray = {emailTemplateAgentLoc,emailTemplateClientLoc,emailReminderTemplateAgentLoc,emailReminderTemplateClientLoc};
String [] smsArray = {smsTemplateAgentLoc,smsTemplateClientLoc,smsReminderTemplateAgentLoc,smsReminderTemplateClientLoc};

java.util.List<String> emailList = new java.util.ArrayList<>();
java.util.List<String> smsList = new java.util.ArrayList<>();

for (String email : emailArray) {
    if (email != null && !email.isEmpty()) {
        emailList.add(email);
    }
}

for (String sms : smsArray) {
    if (sms != null && !sms.isEmpty()) {
        smsList.add(sms);
    }
}

if(emailList.size() > 0) {
    kcontext.setVariable("isEmailRequired",true);
    kcontext.setVariable("emailTemplates",emailList);
}

if(smsList.size() > 0) {
    kcontext.setVariable("isSmsRequired",true);
    kcontext.setVariable("smsTemplates",smsList);    
}

System.out.println("cid:"+kcontext.getVariable("cid")+",node:"+kcontext.getNodeInstance().getNodeName()+",process:"+kcontext.getProcessInstance().getProcessName());]]></bpmn2:script>
